{
  "version": 2,
  "builds": [
    {
      "src": "src/**",
      "use": "@vercel/vite",
      "config": {
        "root": ".",
        "buildCommand": "npm run build",
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": "200"
    }
  ],
  "env": {
    "NODE_VERSION": "18.x"
  }
}